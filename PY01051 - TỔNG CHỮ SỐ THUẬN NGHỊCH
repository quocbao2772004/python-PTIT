import sys
import functools
# fi=open('input.txt','r')
# sys.stdin=fi
import math
from math import sqrt,gcd,log2,log


def main():
    test=int(input())
    for _ in range(test):
        n=input()
        sum=0
        for i in n:
            sum+=int(i)
        if len(str(sum))>1 and str(sum)==str(sum)[::-1]:
            print("YES")
        else:
            print("NO")
if __name__=='__main__':
    main()
