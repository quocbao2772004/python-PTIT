import sys
import functools
# fi=open('input.txt','r')
# sys.stdin=fi
import math
from math import sqrt,gcd,log2,log
def check(s):
    for i in range(0,len(s)-1):
        if s[i]>s[i+1]:
            return 0
    return 1
def main():
    test=int(input())
    for _ in range(test):
        s=input()
        print("YES") if check(s) else print("NO")
if __name__=='__main__':
    main()
