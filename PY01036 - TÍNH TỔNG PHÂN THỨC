# import sys
# fi=open('input.txt','r')
# sys.stdin=fi


def main():
    test=int(input())
    for _ in range(test):
        n=int(input())
        sum=0
        if n%2==1:
            c=1
        else:
            c=2
        for i in range(c,n+1,2):
            sum=sum+1/i
        print("{:.6f}".format(sum))

if __name__=='__main__':
    main()
