import sys
import functools
# fi=open('input.txt','r')
# sys.stdin=fi
# fo=open('output.txt','w')
# sys.stdout=fo
import math
from math import sqrt,gcd,log2,log
from decimal import Decimal, ROUND_HALF_UP
def solve(s):
    n=len(s)//2
    a=s[:n]
    b=s[n:]
    return str(int(a)+int(b))
def main():
    s=input()
    # print(solve(s))
    while len(s)>1:
        s=solve(s)
        print(s)


if __name__=='__main__':
    main()
