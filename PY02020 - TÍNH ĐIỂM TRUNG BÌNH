# import sys
# fi=open('input.txt','r')
# sys.stdin=fi
from decimal import Decimal
import math
from math import sqrt,gcd


def main():
    n=int(input())
    a=list(map(float,input().split()))
    mi=min(a)
    ma=max(a)
    sum=0
    c=0
    for i in a:
        if i!=mi and i !=ma:
            sum=sum+i
            c+=1
    print("{:.2f}".format(sum/c))
if __name__ == '__main__':
    main()
