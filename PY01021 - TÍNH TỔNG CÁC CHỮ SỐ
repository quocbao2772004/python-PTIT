# mongon ngu C.n thcs2 hoc de
def main():
    test=int(input())
    for _ in range(test):
        s=input()
        a=[]
        sum=0
        for i in s:
            if i.isalpha():
                a.append(i)
            else:
                sum=sum+int(i)
        a.sort()
        for i in a:
            print(i,end='')
        print(sum)


if __name__=='__main__':
    main()
