# import sys
# fi=open('input.txt','r')
# sys.stdin=fi
def main():
    test=int(input())
    for _ in range(test):
        n=input()
        if len(n)>=2 and n[-1]=='6' and n[-2]=='8':
            print("YES")
        else:
            print("NO")
if __name__=='__main__':
    main()
    # fi.close()
