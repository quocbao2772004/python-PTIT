import math
from math import sqrt,gcd,log2,log

def main():
    test=int(input())
    for _ in range(test):
        s=input()
        for i in range(1,len(s),2):
            for j in range(int(s[i])):
                print(s[i-1],end='')
        print()
if __name__=='__main__':
    main()
