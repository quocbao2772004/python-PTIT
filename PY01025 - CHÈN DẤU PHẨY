# sys.stdin=fi
import math
from math import sqrt,gcd,log2,log

def main():
    s=input()
    c=0
    res=""
    for i in range(len(s)-1,-1,-1):
        c+=1
        res=s[i]+res
        if c==3 and i!=0:
            c=0
            res=','+res
    print(res)
if __name__=='__main__':
    main()
